{
    "name": "quinzical-api",
    "version": "1.0.0",
    "scripts": {
        "dev": "nodemon --exec babel-node src/index.js",
        "clean": "rm -rf build && mkdir build",
        "build-babel": "babel -d ./build ./src -s",
        "build": "npm run clean && npm run build-babel",
        "start": "npm run build && node ./build/index.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.1",
        "@babel/node": "^7.10.5",
        "@babel/preset-env": "^7.11.0",
        "eslint": "^7.6.0",
        "nodemon": "^2.0.4"
    },
    "dependencies": {
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.10.8"
    }
}
